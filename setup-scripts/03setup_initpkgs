#!/bin/bash
echo "Initial Setup"
cd
yay -S --noconfirm --needed zstd curl gcc make acpid arch-audit linux-firmware btrfs-progs networkmanager pkgconf plymouth efibootmgr wget curl texinfo openssh

sudo cp -R $HOME/git/arch-setup/preconfig-files/sshd_config /etc/ssh/

echo "Finished Installing Initial Packages"

#sudo reboot
